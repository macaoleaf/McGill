public class Cipher{
  public static void main(String[] args){
    char a = args[0].charAt(0);
    int n = Integer.parseInt(args[1]);
    charRightShift(a, n);
  }
  public static char charRightShift(char a, int n){
    char result = 0;
    if(n >= 0 && n <= 25){
      if(){
        result = (a + n);
      }else{
        result = a;
      }
    }else{
      System.out.println("Invalid number entered!");
    }
    return result;
  }
}